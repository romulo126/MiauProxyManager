{
    "manifest_version": 3, 
    "name": "Miau Proxy Manager Simples", 
    "version": "1.0", 
    "description": "Manage multiple proxy profiles easily.", 
    "permissions": [
        "proxy", 
        "storage", 
        "tabs"
    ], 
    "background": {
        "service_worker": "background.js"
    }, 
    "action": {
        "default_popup": "popup.html", 
        "default_icon": {
            "16": "icon16.png",
            "48": "icon48.png", 
            "128": "icon128.png"
        }
    }, 
    "icons": {
        "16": "icon16.png", 
        "48": "icon48.png", 
        "128": "icon128.png"
    }
}